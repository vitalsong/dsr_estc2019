{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/stm_spl/CMSIS/inc",
                "${workspaceFolder}/stm_spl/CMSIS/ST/inc",
                "${workspaceFolder}/stm_spl/CMSIS/ST/inc",
                "${workspaceFolder}/stm_spl/STM32F4xx/inc",
                "${workspaceFolder}/common/include"
            ],
            "defines": [
                "USE_STDPERIPH_DRIVER",
                "STM32F40_41xxx",
                "HSE_VALUE=8000000"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c89",
            "cppStandard": "c++98",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-target=arm-none-eabi",
                "-mcpu=cortex-m4",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard"
            ]
        }
    ],
    "version": 4
}